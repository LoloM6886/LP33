$(function() {
	$('#nav li').hover(function() {
		$(this).children('ul').css({
			visibility: 'visible'
		}).fadeIn(600);
	}, function() {
		$(this).children('ul').stop().hide();

	});
	
});


$(function(){
	$('.fade').cycle();
	
	WebFontConfig = {
		google: { families: [ 'Raleway::latin' ] }
		};
});